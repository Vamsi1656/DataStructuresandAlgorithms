package BinarySearch;

public class Demo {
	
	public static int BinarySearch(int[] nums,int target) {
		
		int left=0;
		int right= nums.length-1;
		
		while(left <=right) {
			int mid=(left+right)/2;
			
			if(mid == target) {
				return mid;
			}
			else if(nums[mid] <target) {
				left = mid+1;
			}
			else
				right=mid-1;
		}
		
		return -1;
	}

}
