package LSBS;

public class Demo {
	
	public static int LinearSearch(int num[],int target) {
		
		for(int i=0;i<num.length;i++) {
			if(num[i]==target)
				return i;
		}
		
		return -1;
	}
	
	public static int BinarySearch(int arr[],int target) {
		
		int left=0;
		int right=arr.length-1;
		
		while(left<=right) {
			int mid = (left+right)/2;
			if(arr[mid]==target) {
				return mid;
			}
			else if(arr[mid]< target) {
				left=mid+1;
			}
			else 
				right = mid-1;
		}
		
		return -1;
		
	}

}
